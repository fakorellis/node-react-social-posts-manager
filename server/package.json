{
    "name": "social-posts-manager",
    "version": "1.0.0",
    "description": "Full-stack web application for managing social posts, authentication, and interactions.",
    "main": "dist/index.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only --watch src src/index.ts",
        "start": "node dist/index.js",
        "build": "tsc",
        "lint": "eslint . --ext .ts",
        "format": "prettier --write .",
        "test": "jest --coverage",
        "docker:build": "docker build -t social-posts-manager .",
        "docker:run": "docker-compose up --build"
    },
    "dependencies": {
        "@type-cacheable/core": "^14.1.0",
        "@type-cacheable/lru-cache-adapter": "^18.0.0",
        "axios": "^1.8.1",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "helmet": "^7.0.0",
        "jsonwebtoken": "^9.0.0",
        "lodash": "^4.17.21",
        "lru-cache": "^11.0.2",
        "mongoose": "^7.2.2",
        "node-cache": "^5.1.2",
        "openapi-backend": "^5.10.6",
        "pino": "^9.6.0",
        "pino-pretty": "^13.0.0",
        "swagger-ui-express": "^5.0.1",
        "winston": "^3.8.2",
        "yamljs": "^0.3.0"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.17",
        "@types/jsonwebtoken": "^9.0.1",
        "@types/lodash": "^4.17.16",
        "@types/mongoose": "^5.11.97",
        "@types/node": "^20.2.5",
        "@types/pino": "^7.0.5",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^29.6.0",
        "prettier": "^3.0.0",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.1.3"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "license": "MIT"
}
